{
  "buildCommand": "pnpm build",
  "outputDirectory": "./dist/public",
  "installCommand": "pnpm install",
  "framework": "vite",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
